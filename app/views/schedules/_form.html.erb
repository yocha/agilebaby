<%= simple_form_for(@schedule) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :age %>
    <%= f.input :data_type %>
    <%= f.input :category_id %>
    <%= f.input :description %>
    <%= f.input :frequency %>
    <%= f.input :required %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
